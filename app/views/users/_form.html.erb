<%= form_for @user do |f| %>
  <%= render 'shared/error_messages' %>
  
  <div>
    <%= f.label :nickname %>
    <%= f.text_field :nickname %>
  </div>
  
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  
  <div>
    <%= f.label :password_confirmation, "Confirmation" %>
    <%= f.password_field :password_confirmation %>
  </div>
    
  <%= f.submit %>
<% end %>
