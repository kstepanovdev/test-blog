<h1 id="form">Log In</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in" %>
    <% end %>
    <div> New user? <%= link_to "Sign up now!", signup_path %> </div>

  </div>
</div>